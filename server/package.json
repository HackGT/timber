{
  "name": "timber-server",
  "version": "0.0.1",
  "private": true,
  "main": "src/app.ts",
  "scripts": {
    "build": "gulp",
    "dev": "nodemon --watch 'src/**/*' --exec 'ts-node' src/app.ts",
    "lint": "eslint src/ --fix; prettier src/ --write",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma-generate": "prisma format && prisma generate",
    "start": "node dist/app.js",
    "start-migrate": "yarn migrate:deploy && yarn start"
  },
  "eslintConfig": {
    "extends": "@hex-labs/eslint-config"
  },
  "prettier": "@hex-labs/prettier-config",
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "@quixo3/prisma-session-store": "^3.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "prettier": "^2.2.1",
    "prisma": "^2.19.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@hex-labs/eslint-config": "^1.1.1",
    "@hex-labs/prettier-config": "^1.1.1",
    "@hex-labs/tsconfig": "^1.1.1",
    "@types/compression": "1.7.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/morgan": "^1.9.2",
    "@types/node-fetch": "^2.5.8",
    "@types/passport": "^1.0.6",
    "@types/passport-oauth2": "^1.4.10",
    "del": "^6.0.0",
    "eslint": "^7.22.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
